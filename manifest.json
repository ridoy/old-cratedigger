{
    "manifest_version": 2,
    "name": "CrateDigger",
    "description": "Turn YouTube into the ultimate music sampling machine",
    "version": "1",
    "author": "Ridoy",
    "content_scripts": [{
        "matches": ["*://*.youtube.com/*"],
        "js": [
            "vendor/jquery-3.3.1.min.js",
            "vendor/arrive.min.js",
            "js/inject.js"
        ],
        "css": [
            "styles/styles.css"
        ]
    }],
    "permissions": [
        "https://*/*",
        "http://*/*",
        "activeTab", "storage", "tabs"
    ]
}
